declare -a ubuntu_version=("20.04" "22.04")
declare -a python_version=("3.8" "3.11")

for ubuntu_version in "${ubuntu_version[@]}"; do
    for python_version in "${python_version[@]}"; do
        echo "${ubuntu_version}:${python_version}";
        #docker build -t "${ubuntu_version}-${python_version}:0.0.0" \
        #             --build-arg UBUNTU_VERSION=$ubuntu_version \
        #             --build-arg PYTHON_VERSION=$python_version \
        #             .
        
    done
done

